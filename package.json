{
  "name": "emily-quinton",
  "description": "Personal website for Emily Quinton",
  "version": "1.0.0",
  "author": "Stef Lewandowski <stef@stef.io>",
  "dependencies": {
    "@artsy/fresnel": "^1.1.0",
    "gatsby": "4.25.7",
    "gatsby-background-image": "^0.10.2",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-manifest": "2.2.42",
    "gatsby-plugin-offline": "3.0.35",
    "gatsby-plugin-postcss": "2.1.20",
    "gatsby-plugin-purgecss": "4.0.1",
    "gatsby-plugin-react-helmet": "3.1.22",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-source-filesystem": "^2.1.48",
    "gatsby-transformer-sharp": "^2.3.14",
    "intersection-observer": "^0.7.0",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-icons": "^3.9.0",
    "typeface-lora": "^0.0.72"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss",
    "emily",
    "quinton"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop --port 6060",
    "dev": "gatsby develop --port 6060",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "autoprefixer": "9.7.4",
    "babel-eslint": "10.0.3",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-react": "7.18.3",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "1.19.1",
    "tailwindcss": "1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stefl/emily-quinton"
  },
  "bugs": {
    "url": "https://github.com/stefl/emily-quinton/issues"
  }
}
